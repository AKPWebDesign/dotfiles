{
  "files.associations": {
    "*": "shellscript",
    "*.md": "markdown"
  },
  "files.trimTrailingWhitespace": false,
  "editor.trimAutoWhitespace": false,
  "files.trimFinalNewlines": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/.vscode/**": true
  }
}
